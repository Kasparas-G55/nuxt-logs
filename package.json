{
  "name": "nuxt-logs",
  "version": "1.0.0",
  "description": "Global Logger for Nuxt",
  "repository": "Kasparas-G55/nuxt-logs",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build && npm run client:build",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "npm run play:dev",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare client",
    "prepare": "nuxi prepare client",
    "play:dev": "nuxi dev playground",
    "play:prod": "npm run prepack && nuxi dev playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.6.5",
    "@nuxt/kit": "^4.1.2",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.13",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/devtools-ui-kit": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.2",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "@vueuse/core": "^13.9.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.36.0",
    "nuxt": "^4.1.2",
    "typescript": "~5.9.2",
    "unocss": "^66.5.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
